alias aliases="vim ~/.aliases"
alias b='bundle'
alias bx='bundle exec'
alias c='open -a Console'
alias cscreen='~/Development/cscreen'
alias cpd='bundle exec cap production deploy'
alias db='rake db:migrate'
alias dotfiles='cd ~/Dropbox/dotfiles'
alias d='cd ~/Development && ls'
alias dns='sudo discoveryutil mdnsflushcache'
alias g='git'
alias gcm='git commit -m'
alias hide="defaults write com.apple.finder AppleShowAllFiles" `#add yes or no`
alias ic="cd ~/Library/Mobile\ Documents/com~apple~CloudDocs/"
alias j="jekyll"
alias jb="jekyll build -w"
alias js="jekyll serve -w"
alias l='cd ~/Development/lakecinema'
alias norm='wget http://necolas.github.io/normalize.css/3.0.2/normalize.css'
alias r='rails'
alias rc='vim ~/.bash_profile'
alias rn='~/Dropbox/Scripts/renamer.sh'
alias scripts="cd ~/Dropbox/Scripts && ls"
alias sr='source ~/.bash_profile'
alias usbdd='sudo killall -STOP -c usbd'
alias v='vim'
alias vi="echo 'use v'"
alias yt="youtube-dl -x --audio-format wav"
