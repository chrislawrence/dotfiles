source ~/Dropbox/dotfiles/aliases.sh
# Global #

alias ng="sudo service nginx"
alias nglog="cd /opt/nginx/logs && ls"
alias sites="cd /opt/nginx/sites-enabled"

# Whittled #

alias w="cd ~/apps/whittled && ls"

# Lake Cinema #

alias l="cd ~/apps/lakecinema && ls"

# Chris Lawrence #

alias c="cd ~/Dropbox/chrislawrence"
